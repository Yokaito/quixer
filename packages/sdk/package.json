{
  "name": "@quixer/sdk",
  "version": "0.0.16",
  "description": "A lightweight, framework agnostic api to use in e-commerce",
  "author": "Guilherme Fontes <gui.fontes.amorim@gmail.com> (@guifontes)",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yokaito/quixer",
    "directory": "packages/sdk"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "default": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./shopify": {
      "import": "./dist/clients/shopify/index.mjs",
      "module": "./dist/clients/shopify/index.mjs",
      "default": "./dist/clients/shopify/index.mjs",
      "types": "./dist/clients/shopify/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@trpc/next": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "@types/node": "^20.9.4",
    "eslint-config-custom": "workspace:*",
    "next": "14.0.3-canary.9",
    "superjson": "^2.2.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.0.1",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@trpc/next": "^10.0.0",
    "@trpc/server": "^10.0.0",
    "next": "^14.0.0"
  }
}
