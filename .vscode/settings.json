{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true,
    "source.sortMembers": true
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "cSpell.words": [
    "healthcheck",
    "metafield",
    "Metafield",
    "metafields",
    "metaobject",
    "quixer",
    "Quixer",
    "shopify",
    "superjson",
    "trpc",
    "TRPC",
    "typesafety",
    "vtex",
    "Vtex"
  ]
}
